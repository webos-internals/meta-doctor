    if [ -f ./build/${DOCTOR}/210/rootfs/usr/lib/ipkg/info/$PACKAGE.list ] ; then \
      cat ./build/${DOCTOR}/210/rootfs/usr/lib/ipkg/info/$PACKAGE.list | \
