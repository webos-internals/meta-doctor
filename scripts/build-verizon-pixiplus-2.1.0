mkdir -p ./build/${DOCTOR}/210/files/boot ./build/${DOCTOR}/210/rootfs 
cp ./build/${DOCTOR}/210/e4014e63a755ac02a63478047ad21d7e.tar ./build/${DOCTOR}/210/nova-cust-image-pixie.rootfs.tar

